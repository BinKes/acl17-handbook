@InProceedings{WiNLP-046,
  author    = {McCurdy, Katherine  and  Serbet\c{c}i, O\u{g}uz},
  title     = {Grammatical gender associations outweigh topical gender bias in crosslinguistic word embeddings},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {1--4},
  abstract  = {Recent research has demonstrated that the relations captured in vector space
	semantic models can reflect undesirable biases in human culture. Beyond the
	semantically topical gender stereotypes observed for English (e.g. a systemic
	male-female:career-family association), the “sheer weight of repetition”
	(Phillips and Boroditsky, 2003) of grammatical gender marking could have
	profound effects on the resulting learned vector representations. The research
	presented here explores the interaction between grammatical gender and topical
	gender bias; key findings include the substantially larger effect of
	grammatical gender relative to topical semantic bias, and the potential of
	lemmatization to reduce gender effects in word embeddings.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2101}
}

@InProceedings{WiNLP-009,
  author    = {Chen, Yun-Nung  and  Hakkani-Tur, Dilek  and  Tur, Gokhan  and  Gao, Jianfeng  and  Celikyilmaz, Asli  and  Deng, Li},
  title     = {Contextual and Structural Language Understanding in Dialogues},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {5--7},
  abstract  = {Natural language understanding (NLU) is a core component of a dialogue system.
	Recently recurrent neural networks (RNN) obtained strong results on NLU due to
	their superior ability of preserving sequential information over time. 
	Traditionally, the NLU module ignores the contexts of utterances and tags
	semantic slots for utterances considering their flat structures, as the
	underlying RNN structure is a linear chain.
	However, contexts and linguistic properties provide informative cues for better
	understanding.
	This paper introduces a novel model, a generalization of RNN to additionally
	incorporate 1) contextual utterances and 2) non-flat network topologies guided
	by prior knowledge. 
	The model automatically figures out the salient and accurate contexts and
	substructures that are essential to predict the semantic tags of the given
	sentences, resulting in better understanding.
	The experiments showed the significant improvement on contextual and structural
	language understanding scenarios.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2102}
}

@InProceedings{WiNLP-063,
  author    = {Novikova, Jekaterina  and  Du\v{s}ek, Ond\v{r}ej  and  Rieser, Verena},
  title     = {Data-driven Natural Language Generation: Paving the Road to Success},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {8--10},
  abstract  = {We argue that there are currently two major bottlenecks to the commercial use
	of statistical machine learning approaches for natural language generation
	(NLG): (a) The lack of reliable automatic evaluation metrics for NLG, and (b)
	The scarcity of high quality in-domain corpora. We address the first problem by
	thoroughly analysing current evaluation metrics and motivating the need for a
	new, more reliable metric. The second problem is addressed by presenting a
	novel framework for developing and evaluating a high quality corpus for NLG
	training.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2103}
}

@InProceedings{WiNLP-081,
  author    = {Yang, Diyi},
  title     = {Persuading People to Give: Modeling Persuasive Strategies via Semi Supervised Neural Nets},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {11--14},
  abstract  = {Persuasive requests that seek to elicit desired responses from their receivers
	are critical to a wide range of fields. However, how to quantify the
	persuasiveness of requests and extract persuasive strategies that lead people
	to satisfy such requests are largely unclear. Building on theories of
	persuasion, in this work, we propose a novel semi-supervised hierarchical
	neural network to identify the persuasive strategies contained in advocation
	requests.  Experiment results show that our proposed semi-supervised
	architecture outperforms several baselines significantly.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2104}
}

@InProceedings{WiNLP-061,
  author    = {Nagarajan, Tushar  and  Jat, Sharmistha  and  Talukdar, Partha},
  title     = {CANDiS: Coupled \& Attention-Driven Neural Distant Supervision},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {15--18},
  abstract  = {Distant Supervision for Relation Extraction uses heuristically aligned text
	data with an existing knowledge base as training data. The unsupervised nature
	of this technique allows it to scale to web-scale relation extraction tasks, at
	the expense of noise in the training data. Previous work has explored
	relationships among instances of the same entity-pair to reduce this noise, but
	relationships among instances across entity-pairs have not been fully
	exploited. We explore the use of inter-instance couplings based on verb-phrase
	and entity type similarities. We propose a novel technique, CANDiS, which casts
	distant supervision using inter-instance coupling into an end-to-end neural
	network model. CANDiS incorporates an attention module at the instance-level to
	model the multi-instance nature of this problem. CANDiS outperforms existing
	state-of-the-art techniques on a standard benchmark dataset.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2105}
}

@InProceedings{WiNLP-037,
  author    = {Jomaa, Hadi  and  Rizk, Yara  and  Awad, Mariette},
  title     = {Semantic and Visual Cues for Humanitarian Computing of Natural Disaster Damage Images},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {19--21},
  abstract  = {Identifying types of damage is essential in times of natural disasters, where
	the internet is flooded with often annotated images and texts, and rescue teams
	are overwhelmed to prioritize often scarce resources. While most of the
	resource allocation efforts rely heavily on human labor, we propose a novel
	hybrid approach for humanitarian computing. Our framework merges low-level
	visual features with semantic attributes obtained after comparing the picture
	annotation to some bag of words. Support vector machine (SVM) was trained on
	these features to classify images as infrastructure or nature damage and tested
	on a dataset gathered from the SUN database and Google Image search. Appending
	semantic attributes resulted in a 5\% increase in classification accuracy to
	95.5\% using linear SVM and motivates an updated folk statement ``an ANNOTATED
	image is worth a thousand word".},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2106}
}

@InProceedings{WiNLP-040,
  author    = {Paula, Felipe  and  Wilkens, Rodrigo  and  Idiart, Marco  and  Villavicencio, Aline},
  title     = {Detecting clinical conditions with Distributional Semantic Models},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {22--25},
  abstract  = {In this paper we propose an approach
	to help detection of neuropsychiatric diseases
	using semantic verbal fluency test
	data, based on the automatic identification
	of semantic clusters. For this task, we investigate
	three approaches based on distributional
	semantic information to determine
	if two consecutive words belong to
	the same cluster or not. We build several
	classifiers and compare their performance,
	discussing feature selection. The overall
	results indicate that for clinical condition
	detection the classifiers that use distributional
	semantics outperform those that
	use the gold standard taxonomy to determine
	clusters, as well as detection based
	on complex network models.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2107}
}

@InProceedings{WiNLP-018,
  author    = {Oraby, Shereen  and  Walker, Marilyn},
  title     = {Chasing Rhetorical Cues to Sarcasm in Online Debate},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {26--29},
  abstract  = {Rhetorical questions are frequently used in social media, aimed as capturing an
	audience's attention or achieving a pragmatic goal, such as argumentation or
	persuasion. Although it has been observed that many of the uses of rhetorical
	questions are sarcastic, there is no large-scale computational work dedicated
	to distinguishing sarcastic from argumentative rhetorical questions, to our
	knowledge. To fill this gap, we expand on the only small existing dataset to
	include 2.5K rhetorical questions in debate forums. We use an LSTM model to
	distinguish between the classes, beating previous baselines with up to 0.75F
	for sarcastic and 0.76F for argumentative, then analyze characteristic
	linguistic categories by class.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2108}
}

@InProceedings{WiNLP-002,
  author    = {Moreira, Viviane  and  Villavicencio, Aline  and  Prates de Pelle, Rogers},
  title     = {Offensive Comments in the Brazilian Web: a dataset and baseline results},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {30--33},
  abstract  = {Brazilian Web users are among the most active in social networks and very keen
	on interacting with others. Offensive comments, known as hate speech, have been
	plaguing online media and originating a number of lawsuits against companies
	which publish Web content. Given the massive number of user generated text
	published on a daily basis, manually filtering offensive comments becomes
	infeasible. The identification of offensive comments can be treated as a
	supervised classification task. In order to obtain a model to classify
	comments, an annotated dataset containing positive and negative examples is
	necessary. The lack of such a dataset in Portuguese, limits the development of
	detection approaches for this language. In this paper, we describe how we
	created annotated datasets of offensive comments for Portuguese by collecting
	news comments on the Brazilian Web. In addition, we provide classification
	results achieved by standard classification algorithms on these datasets which
	can serve as baseline for future work on this topic.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2109}
}

@InProceedings{WiNLP-003,
  author    = {Ross, Candace  and  Barbu, Andrei  and  Katz, Boris},
  title     = {Planning in Language Grounded in Perception},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {34--35},
  abstract  = {We present a novel approach to solving planning problems in artificial
	intelligence by grounding language in perception using a generative model.
	Planning problems, constraints, and plans themselves are generally described in
	a planning language, for example STRIPS or PDDL, providing a particular
	language bias. Our model plans directly in a natural language, English, and it
	specifies the initial conditions, constraints, and final conditions in a
	combination of natural language utterances and videos. Our aim is to solve
	real-world planning problems that might be encountered by a robot and to learn
	to solve such problems from videos of agents performing actions.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2110}
}

@InProceedings{WiNLP-005,
  author    = {Mulki, Hala},
  title     = {Empirical Evaluation of Named Entities on Arabic Sentiment Analysis of Levantine Dialect on Social Media},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {36--38},
  abstract  = {The recent growth of Levantine dialect on social media has been mainly caused
	by the ongoing Syrian crisis. Sentiment analysis of Levantine opinions is
	therefore becoming crucial as it shed the light on the reactions of all the
	sides affected by the ongoing events in Syria. Major events are often related
	to specific persons, locations or organizations so called Named Entities (NEs).
	Here, we propose a sentiment analysis model Tw-StAR in which NEs are employed
	in the sentiment classification process. We hypothesize that sentiment polarity
	information of a named entity is an important information. Hence, our model
	determines the polarity of NEs and involves them in classifying the sentiment
	embedded in Levantine social posts. Tw-StAR has been applied on two Levantine
	datasets of social media content. Results showed that employing NEs improves
	the performance of sentiment classification especially when the lexicon-based
	approach is used.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2111}
}

@InProceedings{WiNLP-006,
  author    = {Rao, Sudha  and  Daum\'{e} III, Hal},
  title     = {Are you asking the right questions? Teaching Machines to Ask Clarification Questions},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {39--42},
  abstract  = {Inquiry is fundamental to communication, and machines cannot effectively
	collaborate with humans unless they can ask questions. We study the problem of
	question generation using data from StackExchange, a plentiful online resource
	in which people routinely ask clarifying questions to posts so that they can
	better offer assistance to the original poster. We build neural network models
	inspired by the idea of the expected value of perfect information: a good
	question is one whose expected answer is going to be most useful. Our results
	demonstrate significant improvements by modeling the value of information.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2112}
}

@InProceedings{WiNLP-007,
  author    = {Peled, Lotem  and  Reichart, Roi},
  title     = {Sarcasm SIGN: Interpreting Sarcasm with Sentiment Based Monolingual Machine Translation},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {43--46},
  abstract  = {Sarcasm is a form of speech in which speakers say the opposite of what they
	truly mean in order to convey a strong sentiment. In other words, ”Sarcasm is
	the giant chasm between what I say, and the person who doesn’t get it.”. 
	In this paper we present the novel task of sarcasm interpretation, defined as
	the generation of a non-sarcastic utterance conveying the same message as the
	original sarcastic one. We introduce a novel dataset of 3000 sarcastic tweets,
	each interpreted by five human judges. Addressing the task as monolingual
	machine translation (MT), we experiment with MT algorithms and evaluation
	measures.
	We then present SIGN: an MT based sarcasm interpretation algorithm
	that targets sentiment words, a defining element of textual sarcasm. We show
	that while the scores of n-gram based automatic measures are similar for all
	interpretation models, SIGN’s interpretations are scored higher by humans for
	adequacy and sentiment polarity. We conclude with a discussion on future
	research directions for our new task.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2113}
}

@InProceedings{WiNLP-010,
  author    = {Cheon, Min-Ah  and  Kim, Chang-Hyun  and  Kim, Jae-Hoon  and  Noh, Eunhee  and  Song, Mi-Young  and  Park, Jong-Im  and  Kim, Yuhyang  and  Lee, Do-Gil},
  title     = {Classify and Examine : Using Semi-supervised Learning with Ensemble Learning to Score Short-Answer Questions in Korean},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {47--50},
  abstract  = {We propose a sentence-level Korean automated scoring tool using a
	semi-supervised learning with ensemble learning to ensure scoring consistency
	and to save labeling efforts for training a scoring classification. The tool
	consists of two phases: language processing and scoring processing. The former
	analyzes student answers using natural language processing techniques: sentence
	segmentation, spacing normalization, spelling correction, POS tagging and
	dependency parsing. The latter repeats the following steps until the prediction
	probability reaches almost 0.0\%. First, scored answers are regarded as initial
	training data. Second, features are extracted from the training data to teach
	ensemble classifiers. Third, unscored answers are classified when their
	prediction probabilities exceed a certain threshold. The classified answers are
	clustered by density so that a human rater can examine them effectively.
	Finally, the human rater confirms them and the confirmed answers are added to
	the training data. Through experiments, we have shown that the Pearson’s
	correlation coefficient between the results of the tool and those of human
	raters are 0.941 in the strong positive range. As a result, our proposed tool
	is practical and useful. In the future, we will improve the performance of our
	scoring tool using deep learning and make it more user-friendly.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2114}
}

@InProceedings{WiNLP-011,
  author    = {Kann, Katharina  and  Cotterell, Ryan  and  Sch\"{u}tze, Hinrich},
  title     = {Parameter Sharing for Morphological Generation Using Neural Sequence-to-Sequence Models},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {51--53},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2115}
}

@InProceedings{WiNLP-012,
  author    = {Gridach, Mourad},
  title     = {Convolutional Neural Networks for Churn Prediction in Microblogs},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {54--56},
  abstract  = {For brands, gaining new customer is more expensive than keeping an existing
	one. Therefore, the ability to keep customers in a brand is becoming more
	challenging these days.  Churn happens when a customer leaves a brand to
	another competitor. Most of the previous work considers the problem of churn
	prediction using the Call Detail Records (CDRs). In this paper, we use
	micro-posts to classify customers into churny or non-churny. In recent years,
	Deep Neural Networks (DNNs) achieved state-of-the-art in various NLP
	applications. We investigate the use of convolutional neural networks (CNNs) to
	classify customers to churny or non-churny. In addition, we show that using
	pretrained word embeddings to initialize word vectors improve the system
	performance. Experimental results showed that we were able to outperform the
	state-of-the-art results on publicly available Twitter dataset.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2116}
}

@InProceedings{WiNLP-013,
  author    = {Villavicencio, Aline  and  Cordeiro, Silvio  and  Ramisch, Carlos},
  title     = {Using Distributional Semantics to Identify Idiomatic Language},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {57--60},
  abstract  = {The accurate understanding of natural language requires an adequate treatment
	of both single words and larger units. However, expressions of the latter kind,
	such as compound nouns, may display variable levels of idiomaticity: while a
	"brick wall" is a wall that is made of bricks, a "loan shark" is not a fish
	that someone may borrow. Therefore it is important to identify which
	expressions are idiomatic, and which are not, as the latter can be interpreted
	from a combination of the meanings of their component words while the former
	cannot. We discuss the ability of distributional semantic models (DSMs) to
	capture idiomaticity in compounds, without any supervision, by means of a
	large-scale multilingual evaluation of DSMs in French and English.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2117}
}

@InProceedings{WiNLP-015,
  author    = {Chen, Huimin  and  Sun, Maosong  and  Tu, Cunchao  and  Lin, Yankai  and  Liu, Zhiyuan},
  title     = {Neural Sentiment Classification with User and Product Attention},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {61--63},
  abstract  = {Document-level sentiment classification aims to predict user's overall
	sentiment in a document about a product. However, most of existing methods only
	focus on local text information and ignore the global user preference and
	product characteristics. Even though some works take such information into
	account, they usually suffer from high model complexity and only consider
	word-level preference rather than semantic levels. To address this issue, we
	propose a hierarchical neural network  to incorporate global user and product
	information into sentiment classification. Our model first builds a
	hierarchical LSTM model to generate sentence and document representations.
	Afterwards, user and product information is considered via attentions over
	different semantic levels due to its ability of capturing crucial semantic
	components. The experimental results show that our model achieves significant
	and consistent improvements compared to all state-of-the-art methods.
	Author{2}{Affiliation}},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2118}
}

@InProceedings{WiNLP-016,
  author    = {Kalra, Kanika  and  Patwardhan, Manasi  and  Kumar, Rahul  and  Sriraman, Anand  and  Radadia, Purushotam},
  title     = {Adaptive Platform for Collaborative Translation},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {64--68},
  abstract  = {Using crowd for translation is time and cost efficient technique for generating
	large parallel corpora. We have designed and implemented a crowd translation
	platform, which provides flexibility to integrate variety of workflows involved
	in the crowd translation lifecycle to ensure productivity and quality, through
	collaborative task allocation, customizable architecture and effective
	visualizations.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2119}
}

@InProceedings{WiNLP-017,
  author    = {Andy, Anietie  and  Sekine, Satoshi  and  Rwebangira, Mugizi  and  Dredze, Mark},
  title     = {Name Variation in Community Question Answering Systems},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {69--71},
  abstract  = {A common challenge with community question answering systems is that a
	significant percentage of asked questions are left unanswered. In this paper,
	we propose an algorithm to reduce the number of unanswered questions in Yahoo!
	Answers by reusing the answer to the most similar past resolved question to an
	unanswered question, from the site. In this research, we focus on question
	categories in which most questions contain named entities and entity aliases.
	We show that in these categories, entity linking can be used to identify
	relevant past resolved questions with shared needs as a given question by
	disambiguating named entities and matching these questions based on the
	disambiguated entities, identified entities, and knowledge base information
	related to these entities. We evaluated our algorithm on a new dataset
	constructed from Yahoo! Answers.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2120}
}

@InProceedings{WiNLP-021,
  author    = {Tabassum, Jeniya  and  Ritter, Alan  and  Xu, Wei},
  title     = {Time Expression Resolution for Social Media Data},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {72--75},
  abstract  = {We describe TweeTIME, a minimally supervised time resolver for social media
	domain that learns from large quantities of unlabeled data and requires no
	hand-engineered rules or hand-annotated training corpora. Our system achieves
	0.68 F1 score on the end-to-end task of resolving date expressions,
	outperforming a broad range of state-of-the-art systems.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2121}
}

@InProceedings{WiNLP-022,
  author    = {A, Yana  and  Shen, Shiqi  and  Liu, Zhiyuan  and  Sun, Maosong},
  title     = {Neural Headline Generation with Sentence-wise Optimization},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {76--78},
  abstract  = {Recently, neural models have been proposed for headline generation by learning
	to map documents to headlines with recurrent neural networks. Nevertheless, as
	traditional neural network utilizes maximum likelihood estimation for parameter
	optimization, it essentially constrains the expected training objective within
	word level rather than sentence level. Moreover, the performance of model
	prediction significantly relies on training data distribution. To overcome
	these drawbacks, we employ minimum risk training strategy in this paper, which
	directly optimizes model parameters in sentence level with respect to
	evaluation metrics and leads to significant improvements for headline
	generation.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2122}
}

@InProceedings{WiNLP-023,
  author    = {Tomkins, Sabina  and  Xu, Anbang  and  Liu, Zhe  and  Guo, Yufan},
  title     = {Discovering Domain Specific Dialog Acts},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {79--82},
  abstract  = {We introduce the first unsupervised, end-to-end method that discovers and
	quantifies the number of dialogue acts in an open domain,  with the ability to
	generate utterance templates for predicted dialog acts. Particularly, it models
	the sequential behavior of dialog acts as well as the role of emotions in
	conversation. We evaluate the method on customer service conversations on
	Twitter from multiple industries. Our method is successful in detecting novel
	domain specific dialog acts, and in generating appropriate replies to various
	customer service requests.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2123}
}

@InProceedings{WiNLP-024,
  author    = {Xia, Qiaolin  and  Sha, Lei  and  Chang, Baobao  and  Sui, Zhifang},
  title     = {Learning from Heterogeneous Data with PNN for Chinese SRL},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {83--85},
  abstract  = {Previous studies on Chinese semantic role labeling (SRL) have concentrated on a
	single semantically annotated corpus. But the training data of single corpus is
	often limited. Whereas the other existing semantically annotated corpora for
	Chinese SRL are scattered across different annotation frameworks. But still,
	Data sparsity remains a bottleneck. This situation calls for larger training
	datasets, or effective approaches which can take advantage of highly
	heterogeneous data. In this paper, we focus mainly on the latter, that is, to
	improve Chinese SRL by using heterogeneous corpora together. We propose a novel
	progressive learning model which augments the Progressive Neural Network with
	Gated Recurrent Adapters. The model can accommodate heterogeneous inputs and
	effectively transfer knowledge between them. We also release a new corpus,
	Chinese SemBank, for Chinese SRL. Experiments on CPB 1.0 show that our model
	outperforms state-of-the-art methods.
	Author{4}{Affiliation}},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2124}
}

@InProceedings{WiNLP-026,
  author    = {Rizk, Yara  and  Kshoury, Khodor  and  Chehab, Mohamad  and  Chidiac, Petra  and  Awad, Mariette  and  Antoun, Joumana},
  title     = {Virtual Patient},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {86--88},
  abstract  = {In an objective structured clinical examination (OSCE), a standardized patient
	(SP) is trained to enact a specific medical scenario that a medical student
	must diagnosis. Our virtual patient (VP) uses an artificial intelligence system
	to answer the student's queries by extracting syntactic, semantic and bag of
	words (BoW) features from a question and script, classifying them into
	predefined classes using support vector machines (SVM), then finding the best
	match using k-nearest neighbor (kNN). The system was tested on a database of
	200 compiled scenarios and achieved a question-answer matching accuracy of
	78\%. An authoring tool provides medical doctors with a user-friendly interface
	to create medical scenario scripts, and a visualization system renders an
	animated 3D VP to interact with medical students.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2125}
}

