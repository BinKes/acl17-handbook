}

@InProceedings{WiNLP-028,
  author    = {Gella, Spandana},
  title     = {Visual Verb Sense Disambiguation},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {89--91},
  abstract  = {In this paper, we propose 
	a new action recognition task of 
	visual verb sense disambiguation task: given 
	an image and a 
	polysemous verb (which has multiple meanings 
	depending on context), the task is to assign 
	the correct sense of the verb depicted in 
	the image.  We introduce VerSe
	a new dataset that augments existing multimodal 
	datasets (MSCOCO and TUHOI) with verb and sense labels.
	We propose an unsupervised algorithm which
	performs visual sense disambiguation using 
	textual, visual, or multimodal representations. Our 
	results show that multimodal representations help in 
	disambiguating the verb depicted in an image.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2126}
}

@InProceedings{WiNLP-029,
  author    = {Vania, Clara  and  Lopez, Adam},
  title     = {From Characters to Words to in Between: Do We Capture Morphology?},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {92--95},
  abstract  = {Words can be represented by composing the representations of subword units such
	as word segments, characters, and/or character n-grams. While such
	representations are effective and may capture the morphological regularities of
	words, they have not been systematically compared, and it is not understood how
	they interact with different morphological typologies. On a language modeling
	task, we present experiments that systematically vary (1) the basic unit of
	representation, (2) the composition of these representations, and (3) the
	morphological typology of the language modeled. Our results largely confirm
	previous findings that character representations are effective across many
	languages, though we find that a previously unstudied combination of character
	trigram representations composed with bi-LSTMs outperforms most other settings.
	 However, we also find room for improvement: character models do not match the
	predictive accuracy of a model with access to explicit morphological analyses.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2127}
}

@InProceedings{WiNLP-030,
  author    = {Gilroy, Sorcha  and  Lopez, Adam  and  Maneth, Sebastian},
  title     = {Regular Graph Grammars for NLP},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {96--98},
  abstract  = {NLP systems for machine translation, summarization,  paraphrasing, and other
	tasks often fail to preserve the compositional semantics of sentences and
	documents because they model language as bags of words, or at best syntactic
	trees. To preserve semantics, they must model semantics. In pursuit of this
	goal, several datasets have been produced which pair natural language with
	compositional semantic representations in the form of directed acyclic graphs
	(DAGs), including the Abstract Meaning Representation Bank (AMR;
	\citealt{AMR}), the Prague Czech-English Dependency Treebank \cite{pcedt},
	Deepbank \cite{Flickinger+etal:2012:tlt}, and the Universal Conceptual
	Cognitive Annotation \cite{Abend+Rappoport:2013:acl}. To make use of this data,
	we require models of graphs.
	Models of strings and trees can be represented by finite state machines, and
	this representation characterizes many models in natural language processing.
	Due to the many graph datasets which have become available, our first research
	question is: \textbf{Can we define the equivalent of a finite state model for
	graphs?} We are also interested in knowing whether there is a computational
	advantage to using such a model and so our second research question is:
	\textbf{Can we define an efficient parsing algorithm for such a model on
	graphs?} The answer to both of our research questions is yes.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2128}
}

@InProceedings{WiNLP-031,
  author    = {Schofield, Alexandra  and  Magnusson, M\r{a}ns  and  Thompson, Laure  and  Mimno, David},
  title     = {Understanding Text Pre-processing for Latent Dirichlet Allocation},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {99--102},
  abstract  = {To apply natural language modeling techniques to new corpora requires users to
	convert documents to data using various pre-processing treatments. However, the
	effects of these transformations are still poorly understood. We describe
	several studies that quantify the impact of pre-processing in different forms,
	focusing on topic modeling applications. We find that many common practices
	either have no measurable effect or have a negative effect after accounting for
	biases induced by feature selection. Finally, we provide recommendations as to
	how to pre-process text for novice users of topic models looking to investigate
	their own text corpora.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2129}
}

@InProceedings{WiNLP-032,
  author    = {Tutubalina, Elena},
  title     = {Combination of RNN and CRF for Extracting Adverse Drug Reactions from User Reviews},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {103--106},
  abstract  = {Adverse drug reactions (ADRs) are an essential part of the analysis of drug
	use, measuring their benefits, and making policy decisions. Traditional
	channels for identifying ADRs are reliable but very slow and only produce a
	small amount of data. Text reviews, either on specialized web sites or in
	general-purpose social networks, may lead to a data source of unprecedented
	size, but identifying ADRs in free-form text is a challenging natural language
	processing problem. In this work, we propose a novel model for this problem,
	uniting recurrent neural architectures and conditional random fields. We
	evaluate our model with a comprehensive experimental study, showing
	improvements over the state-of-the-art methods.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2130}
}

@InProceedings{WiNLP-033,
  author    = {Yan, Xinru  and  Pedersen, Ted},
  title     = {Who's to say what's funny? A computer using Language Models and Deep Learning, That's Who!},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {107--109},
  abstract  = {Humor is a defining characteristic of human beings. Our goal is to develop
	methods that automatically detect humorous statements and rank them on a
	continuous scale. In this paper we report on results using a Language Model
	approach, and outline our plans for using methods from Deep Learning.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2131}
}

@InProceedings{WiNLP-034,
  author    = {Cotik, Viviana  and  Filippo, Dario  and  Roller, Roland  and  Xu, Feiyu  and  Uszkoreit, Hans},
  title     = {Creation of an Annotated Corpus of Spanish Radiology Reports},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {110--113},
  abstract  = {This paper presents a new annotated corpus of 513 anonymized radiology reports
	written in Spanish. Reports were manually annotated with entities, negation and
	uncertainty terms and relations. The corpus was conceived as an evaluation
	resource for named entity recognition and relation extraction algorithms, and
	as input for the use of supervised methods. Biomedical annotated resources are
	scarce due to confidentiality issues and associated costs. This work provides
	some guidelines that could help other researchers to undertake similar tasks.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2132}
}

@InProceedings{WiNLP-035,
  author    = {Addawood, Aseel},
  title     = {Argumentation Mining In Twitter: A Study Of Controversial Topics},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {114--117},
  abstract  = {In recent years, social media has revolutionized how people communicate and
	share information. One function of social media, besides connecting with
	friends, is sharing opinions with others. Microblogging sites like Twitter have
	often provided an online forum for social activism. When users debate
	controversial topics on social media, they typically share different types of
	evidence to support their claims. Classifying these types of evidence can
	provide an estimate for how adequately the arguments have been supported. We
	first introduce a manually built gold standard dataset of 3,000 tweets related
	to the recent FBI and Apple encryption debate. We develop a framework for
	automatically classifying six evidence types typically used on Twitter to
	discuss the debate. Our findings show that a Support Vector Machine (SVM)
	classifier trained with n-gram and additional features can capture different
	forms of representing evidence on Twitter, exhibiting significant improvements
	over the unigram baseline and achieving a F1 macro-average of 82.8\%.
	Recommendations and insights into this task is also shared in this paper to
	support others working on similar tasks. This work has been published in the
	3rd Workshop on Argument Mining, ACL 2016.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2133}
}

@InProceedings{WiNLP-036,
  author    = {Jomaa, Hadi  and  Kamereddine, Mohamad  and  Nayal, Ammar  and  Rizk, Yara  and  Awad, Mariette},
  title     = {Affective Relationship Between Color and Text in Arabic Comic books},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {118--120},
  abstract  = {Comic books are colorful depictions of annotated events that have increased in
	popularity over the past decade in this digital world. In this paper, we focus
	on the connection between the text and images, comparing the emotions
	separately conveyed by the color of drawings and the text in speech balloons.
	Natural language processing (NLP) techniques extract features from the text to
	train SVM on emotion identification, while color theory is applied to analyze
	the images' emotional implications. Trained on a homegrown database, a
	discrepancy between conveyed emotions in text and images was observed.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2134}
}

@InProceedings{WiNLP-038,
  author    = {Odilinye, Lydia  and  Popowich, Fred},
  title     = {A Hybrid Approach to Recommender Systems for Technology Enhanced Learning: Using Probabilistic Topic Models, Collaborative Tagging and Domain Ontologies},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {121--123},
  abstract  = {We describe on-going work on a hybrid approach to recommender systems that is
	aimed at improving technology enhanced learning (TEL). The recommender system
	leverages the benefits of three different
	techniques: probabilistic topic models, collaborative tagging systems and
	domain ontologies. In this paper, we describe the architecture of the proposed
	system, data sources as well as the processes involved.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2135}
}

@InProceedings{WiNLP-039,
  author    = {Dubremetz, Marie  and  Nivre, Joakim},
  title     = {Syntax Matters for Rhetorical Structure: The Case of Chiasmus},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {124--126},
  abstract  = {The chiasmus is a rhetorical figure involving the repetition of a pair of words
	in reverse order, 
	as in ``all for one, one for all''. Previous work on detecting chiasmus in
	running
	text has only considered superficial features like words and punctuation. In
	this paper, we explore the
	use of syntactic features as a means to improve the quality of chiasmus
	detection. Our results show that 
	taking syntactic structure into account may increase average precision from
	about 40 to 65\% on texts 
	taken from European Parliament proceedings. To show the generality of the
	approach, we also evaluate it on literary text and observe a similar
	improvement and a slightly better overall result.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2136}
}

@InProceedings{WiNLP-041,
  author    = {Chioma, Enemouh  and  Hepple, Mark  and  Ezeani, Ignatius  and  Onyenwe, Ikechukwu},
  title     = {Morph-Inflected Word Detection in Igbo via Bitext},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {127--129},
  abstract  = {Agglutination in a language occurs when
	affixes are combined with a stem or
	word to form variants of morphologically-
	inflected (morph-inflected) words. The
	stem and the associated affixes remain un-
	changed in every aspect after their union,
	and they can represent unit words in other
	languages like English. We, therefore,
	hypothesize that unit words from agglu-
	tinative languages that map to multiple
	unit words in other languages via word
	alignment are likely to be morph-inflected
	words. In this work, we propose a new ap-
	proach to detect possible morph-inflected
	words that uses bitext information from
	Igbo and English.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2137}
}

@InProceedings{WiNLP-042,
  author    = {Tavares, Tatiane  and  Timponi Torrent, Tiago},
  title     = {Partial productivity of The Indefinite Noun Quantification Construction: implications for computational modeling in FrameNet Brasil},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {130--132},
  abstract  = {This work aims to revisit the literature about Construction Grammar and Coinage
	Patterns (Kay, 2013) and discuss to what extent the distinction between these
	two concepts is relevant to the computational modeling of constructions in
	FrameNet Brasil. From Kay’s (2005, 2013) perspective, it is assumed that
	constructions are the more general and productive patterns in a language, while
	coinage patterns are non-productive, difficult to regularize and outside the
	grammar. We seek to analyze whether the Brazilian Portuguese indefinite
	quantity structure [N1 de [N2]] must be taken as a partially productive
	construction or a coinage pattern and to specify the parameters for annotation
	of these linguistic items in a computational model. The analysis is guided by
	the principles of Frame Semantics (Fillmore, 1982, 1985) and Construction
	Grammar (Fillmore et al, 2013; Goldberg, 1995, 2006; Kay, 2013) and by the
	FrameNet methodology.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2138}
}

@InProceedings{WiNLP-043,
  author    = {Naderi, Nona},
  title     = {Identifying frames at the sentence level in news articles},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {133--136},
  abstract  = {Framing is generally conceptualized as a communication process to present an
	object or an issue and is considered as a significant factor in the success of
	argumentation. We present a supervised approach for identifying frames at the
	sentence level. We conduct a set of experiments using LSTMs, BLSTMs, and GRUs
	on the Media Frames Corpus. Our method outperforms several baselines.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2139}
}

@InProceedings{WiNLP-044,
  author    = {Zanon Boito, Marcely  and  Besacier, Laurent  and  Villavicencio, Aline},
  title     = {Unsupervised Word Discovery Using Attentional Encoder-Decoder Models},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {137--140},
  abstract  = {In this project we explore soft-alignment probability matrices generated by an
	attention-based sequence-to-sequence neural machine translation system, in
	order to evaluate if these soft-alignments allow us to discover latent lexicon
	representation. Our goal is to understand if it is possible to use these
	soft-alignments for unsupervised segmentation and lexicon discovery in
	low-resource scenarios, limited by the amount of data available.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2140}
}

@InProceedings{WiNLP-045,
  author    = {Asai, Akari  and  Li, Vivian  and  Stepanov, Daniela  and  Tan, Wang-Chiew},
  title     = {A Data-Driven Approach to Understanding Happiness},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {141--143},
  abstract  = {The science of happiness is an area of positive psychology that studies the
	factors that sustain people's happiness over time. There has been recent
	interest to develop technology that helps users incorporate the findings of the
	science of happiness into their daily lives.
	At the Recruit Institute of Technology, our goal is  to broaden  the  data  we 
	collect  about                                people’s happiness to include text
	that
	describes
	their
	happy
	moments. 
	Understanding such text is crucial for an interactive assistant to be able to
	provide insights that are tailored to the user's specific life circumstances.
	In this abstract, we describe Joybot v1.0, the first version of our happiness
	app that allows a user to record their happy moments via text, voice, or
	photographs on one's mobile device. Joybot interprets these happy moments
	automatically and develops, as happy moments are entered, an understanding of
	the key factors that make the user happy and how. We then describe the
	technical NLP challenges that Joybot raises, and a crowdsourced corpus of happy
	moments that we collected in order to advance the research on NLP for
	happiness.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2141}
}

@InProceedings{WiNLP-047,
  author    = {Peng, Nanyun  and  Dredze, Mark},
  title     = {Multi-task Domain Adaptation for Sequence Tagging},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {144--146},
  abstract  = {Many domain adaptation approaches rely
	on learning cross domain shared representations,
	so that the knowledge learned in
	one domain can efficiently transfer to other
	domains. Traditional domain adaptation
	only considers one task. In this paper, we
	explore multi-task representation learning
	in the domain adaptation scenario. We
	propose a neural network framework that
	can easily conduct domain adaptation for
	multiple tasks simultaneously, and learn
	shared representations that generalize better
	for domain adaptation. We apply the
	proposed framework to domain adaptation
	on sequence tagging problems considering
	two tasks: Chinese word segmentation
	and named entity recognition. Experiments
	show that multi-task domain adaptation
	works better than disjoint domain
	adaptation for each task, and achieves the
	state-of-the-art results for both tasks on social
	media domain.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2142}
}

@InProceedings{WiNLP-049,
  author    = {Xu, Wei  and  Napoles, Courtney  and  Pavlick, Ellie  and  Chen, Quanze  and  Callison-Burch, Chris},
  title     = {Optimizing Statistical Machine Translation for Text Simplification},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {147--150},
  abstract  = {Most recent sentence simplification systems use basic machine translation
	models to learn lexical and syntactic paraphrases from a manually simplified
	parallel corpus. These methods are limited by the quality and quantity of
	manually simplified corpora, which are expensive to build. In this paper, we
	conduct an in-depth adaptation of statistical machine translation to perform
	text simplification, taking advantage of large-scale paraphrases learned from
	bilingual texts and a small amount of manual simplifications with multiple
	references. Our work is the first to design automatic metrics that are
	effective for tuning and evaluating simplification systems, which will
	facilitate iterative development for this task.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2143}
}

@InProceedings{WiNLP-050,
  author    = {Currey, Anna},
  title     = {Copying Monolingual Data Improves Neural Machine Translation},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {151--154},
  abstract  = {We train a neural machine translation (NMT) system to both translate
	source-language text and copy target-language text, thereby exploiting
	monolingual corpora in the target language. Specifically, we create a bitext
	from the monolingual text in the target language by copying the corpus so that
	each source sentence is identical to the target sentence. This copied data is
	then mixed with the parallel corpus and the NMT system is trained like normal,
	with no metadata to distinguish the two input languages. Our method stacks with
	back-translation, even if the same monolingual corpus is used for copying and
	back-translation. In an English-Turkish translation task, we gain 1.2 BLEU
	points over a strong baseline that includes back-translation.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2144}
}

