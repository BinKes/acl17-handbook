  title = {Insights on Sentiment Polarity Extraction from Bollywood Song Lyrics},
  author = {Apoorva, Drushti and Mamidi, Radhika},
  sortname = {Apoorva, Drushti and Mamidi, Radhika}}

@InProceedings{WiNLP-052,
  author    = {Henry, Cassidy  and  Moolchandani, Pooja  and  Pollard, Kimberly A.  and  Bonial, Claire  and  Foots, Ashley  and  Artstein, Ron  and  Hayes, Cory  and  Voss, Clare R.  and  Traum, David  and  Marge, Matthew},
  title     = {Towards Efficient Human-Robot Dialogue Collection: Moving Fido into the Virtual World},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {155--157},
  abstract  = {Our research aims to develop a natural dialogue interface between robots and
	humans. We describe two focused efforts to increase data collection efficiency
	towards this end: creation of an annotated corpus of interaction data, and a
	robot simulation, allowing greater flexibility in when and where we can run
	experiments.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2145}
}

@InProceedings{WiNLP-053,
  author    = {Jha, Akshita  and  Mamidi, Radhika},
  title     = {When does a Compliment become Sexist? Analysis and Classification of Ambivalent Sexism using Twitter Data},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {158--160},
  abstract  = {Sexism is prevalent in today’s society, both offline and online, and poses a
	credible threat to social equality with respect to gender. According to
	ambivalent sexism theory (Glick and Fiske, 1996), it comes in two forms:
	Hostile and Benevolent. While hostile sexism is characterized by an explicitly
	negative attitude, benevolent sexism is more subtle. Previous works on
	computationally detecting sexism present online are restricted to identifying
	the hostile form. Our objective is to investigate the less pronounced form of
	sexism demonstrated online. We achieve this by creating and analyzing a dataset
	of tweets that exhibit benevolent sexism. By using Support Vector Machines
	(SVM), sequence-to-sequence models and FastText classifier, we classify tweets
	into ‘Hostile’, ‘Benevolent’ or ‘Others’ class depending on the
	kind of sexism they exhibit. We have been able to achieve an F1-score of 87.22\%
	using FastText classifier. Our work helps analyze and understand the much
	prevalent ambivalent sexism in social media.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2146}
}

@InProceedings{WiNLP-054,
  author    = {Vincze, Veronika  and  Horv\'{a}th, Csilla},
  title     = {My Husband, the Dragonfly: The (Non-)Compositionality of Mansi Animal Names},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {161--163},
  abstract  = {Here we focus on a specific type of multiword expressions , namely animal names
	in Mansi, an endangered language spoken in Russia. In several cases, animal
	names inherently include a meaning component related to a family term or a
	gender-marking word, thus they may show a high degree of non-compositionality.
	We present our data collected from dictionaries and folk tales of the Mansi
	language and we also demonstrate that machine learning tools can effectively
	learn the distinction between lexically male and female animals.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2147}
}

@InProceedings{WiNLP-055,
  author    = {Liebeskind, Chaya  and  Liebeskind, Shmuel  and  HaCohen-Kerner, Yaakov},
  title     = {Comment Relevance Classification in Facebook},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {164--166},
  abstract  = {Comment relevance classification aims to classify a comment as relevant or
	irrele-vant to the content of its post. Since the comments in social media are
	usually short, their bag-of-words (BoW) represen-tations are highly sparse. In
	this paper, we explore four semantic vector representations for the relevance
	classification task. We investigate different types of large unlabeled data for
	learning the distribu-tional representations. We also empirically demonstrate
	that expanding the input of the task to include the post text does not improve
	the classification performance over using only the comment text.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2148}
}

@InProceedings{WiNLP-058,
  author    = {Jena, Grishma  and  Vashisht, Mansi  and  Basu, Abheek  and  Sedoc, Joao  and  Ungar, Lyle},
  title     = {Enterprise to Computer: Star Trek chatbot},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {167--171},
  abstract  = {Adding a persona to a chatbot is essential for this goal and contributes to a
	better and more engaging user experience. In this work, we propose a design for
	a chatbot that captures the ‘style’ of Star Trek by incorporating
	references from the show along with peculiar tones of the fictional characters
	therein. Enterprise to Computer (E2Cbot) treats Star Trek dialog style and
	general dialog style differently by two recurrent neural network
	Encoder-Decoder models. The Star Trek dialog style uses sequence to sequence
	(SEQ2SEQ) models trained on Star Trek dialogs. The general dialog style uses
	Word Graph to shift the response of the SEQ2SEQ model into the Star Trek
	domain.  To ensure an engaging conversation, we have defined rules to output
	standard responses when the model confidence is low. Our model is able to
	automatically generate text in Star Trek style, even for out of domain input.
	Since we are using a data driven approach, this model can be easily expanded to
	other domains like news or sports. We compare our model with a publically
	available rule based chatbot using perplexity, vocabulary overlap and human
	scores. We foresee a wide variety of applications for the given model. The
	resulting chatbot can be used in various social situations as it can mimic
	different personae.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2149}
}

@InProceedings{WiNLP-059,
  author    = {Knowles, Rebecca  and  Koehn, Philipp},
  title     = {Neural Interactive Translation Prediction},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {172--174},
  abstract  = {We present an interactive translation prediction method based on neural machine
	translation. Even with the same translation quality of the underlying machine
	translation systems, the neural prediction method yields much higher word
	prediction accuracy (61.6\% vs. 43.3\%) than the traditional method based on
	search graphs.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2150}
}

@InProceedings{WiNLP-062,
  author    = {Winata, Genta Indra  and  Kampman, Onno  and  Yang, Yang  and  Fung, Pascale},
  title     = {Psychological Stress Detection from Text Using Word Embeddings},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {175--178},
  abstract  = {We propose methods to classify psychological stress from text, using word2vec
	embeddings as features. The performance of LSTM models with embeddings
	outperform the other models in terms of accuracy (69.7\%). However, TF-IDF
	models still work best in our specific closed-domain corpus. To illustrate use
	cases of such a stress classifier, we build a virtual psychologist called Nora.
	She is aware of user stress state, adapts to it and provides emotional support
	and counseling.
	Author{4}{Affiliation}},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2151}
}

@InProceedings{WiNLP-064,
  author    = {Mager Hois, Jes\'{u}s Manuel  and  Carrillo Gonz\'{a}lez, Di\'{o}nico  and  Meza Ru\'{i}z, Ivan Vladimir},
  title     = {WixNLP: Probabilistic Finite-State morphological analyzer for Wixarika language},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {179--182},
  abstract  = {We present the first morphological analyzer for the Mexican indigenous language
	Wixarika, also known as Huichol. Indigenous languages in Mexico have seldom
	been the focus of NLP. However, with the recent reach of computing technology
	into indigenous communities, this has started to change and has generated a
	growing interest in the topic. An important aspect that these languages share
	is complex agglutinative verbal morphology. In this work, we present a
	morphological analyzer for Wixarika which also holds this property. On top of
	the agglutinative nature of the language, the low amount of resources and the
	lack of a orthographic standard among dialects add to the challenge. Our
	proposal is based on a probabilistic finite-state approach that exploits
	regular agglutinative patterns and requires little linguistic knowledge. We
	show that our approach outperforms unsupervised
	methods in a low-resource context. The dataset used in this work was released
	for future work.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2152}
}

@InProceedings{WiNLP-068,
  author    = {Yeganova, Lana  and  Kim, Won  and  Kim, Sun  and  Islamaj Do\u{g}an, Rezarta  and  Liu, Wanli  and  Comeau, Donald  and  Lu, Zhiyong  and  Wilbur, W John},
  title     = {PubTermVariants: A Data-driven Approach to Finding Biomedical Term Variants},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {183--185},
  abstract  = {Term normalization is frequently used in information retrieval task to reduce
	variant word forms to a common form. The most general term normalization
	tech-nique used in practice is stemming, however it has been found to not be
	completely reliable.  Here we present PubTermVariants, a high-quality
	data-driven resource of term variant pairs that can improve search results in
	PubMed. For a given pair, we consider two terms to be variants if they stem to
	the same form, pass the hypergeometric test, and pass the morpho-semantic test.
	We perform manual evaluation of a subset of PubTermVariants that confirms the
	high quality of the pairs.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2153}
}

@InProceedings{WiNLP-069,
  author    = {El Asri, Layla  and  Trischler, Adam},
  title     = {Safe Evaluation of Dialogue Management},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {186--189},
  abstract  = {This extended abstract presents preliminary work on safe evaluation of the
	policy of a dialogue manager. The dialogue manager is trained through
	reinforcement learning with a user simulator. Safe evaluation takes into
	account the uncertainty over the user simulator's behaviour during training. We
	show that including this uncertainty into the dialogue manager's reward
	function leads to more accurate evaluation and more efficient exploration.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2154}
}

@InProceedings{WiNLP-070,
  author    = {Wang, Nan  and  Song, Yan},
  title     = {What does an effective treatment recommendation look like: A corpus-based study on doctor-patient conversations},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {190--192},
  abstract  = {Doctor-patient communication contributes to improved health outcomes. Some
	language practices of treatment recommendation are related to higher patient
	acceptance rates and satisfaction rates. In this study, we examine physicians'
	treatment recommendations,  caregivers' responses, and their associations based
	on an annotated corpus of 318 pediatric conversations. In future work, our
	objective is to bridge the social scientific research on doctor-patient
	communication and computational research on conversational understanding and
	dialogue systems.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2155}
}

@InProceedings{WiNLP-071,
  author    = {Petruck, Miriam R L  and  Dodge, Ellen K},
  title     = {Linguistic Theories and Resources for Natural Language Processing},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {193--196},
  abstract  = {FrameNet (FN), Embodied Construction Grammar (ECG), and MetaNet (MN), are
	distinct from other linguistic resources since each relies on frame-based
	meaning representation, rather than logics-based representation. This work
	addresses the need for frames, constructions, and metaphor in
	Natural Language Processing (NLP). Importantly, it also shows that
	computational linguistics must appeal to linguistic theory.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2156}
}

@InProceedings{WiNLP-073,
  author    = {Misra, Amita  and  Walker, Marilyn},
  title     = {Argument Summarization in Social Media Dialogue},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {197--200},
  abstract  = {Online debates have a number of important properties
	that potentially make them more engaging
	and persuasive than their offline counterparts, in
	their use of emotional language, social affiliation,
	argument structuring and audience involvement.
	The goal of this work is to extract meaningful information
	from the online debate dialogues that
	can be used to summarize the issues under discussion
	and the basis for disagreements. We propose
	summarization with a focus towards the argument
	propositions and use pyramid tier structure to extract
	these propositions. Our hypothesis is that the
	arguments most central to the dialogue correspond
	to top-tier SCUs and represent the opinionated dialogue
	summary. We present a novel method of
	linking back the top-tier summary contributors to
	the original dialogue sentences and develop a classifier
	that can identify these sentences with an Fmeasure
	of 0.73 for the topic of gun control as
	compared to a lexrank baseline of 0.58},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2157}
}

@InProceedings{WiNLP-074,
  author    = {Thi Lan, Ngo  and  Van Hoang, Bui  and  Khac Linh, Pham  and  Bao Son, Pham  and  Xuan Hieu, Phan},
  title     = {Functional Segment Identification and Emotion Tagging for Vietnamese Conversational Texts},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {201--204},
  abstract  = {In this paper, for effectively extract the emotional smallest part that has a
	complete meaning in communication function in a text, we focus on emotion
	analysis at functional segment level base on ISO 24617-2 standard. Given a
	conversational text, we investigate different strategies, which using machine
	learning based approach, to segment text into functional segments and classify
	each functional segment into one of seven emotion categories (anger, disgust,
	fear, joy, sadness, surprise, and none). Experimental results on a corpus of
	Facebook messages in Vietnamese are very promising. To the best of our
	knowledge, this is the first attempt to analyzes emotion at functional segment
	level.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2158}
}

@InProceedings{WiNLP-075,
  author    = {Asaadi, Shima  and  Rudolph, Sebastian},
  title     = {Learning Sentiment-Aware Word Representation in Compositional Matrix-Space Models},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {205--207},
  abstract  = {Learning word representations to capture the semantics and compositionality of
	language has received much research interest in NLP. In this work, we
	investigate the problem of learning matrix representations of words and present
	a learning approach for compositional matrix-space models for the task of
	fine-grained sentiment analysis. We find that our approach outperforms other
	approaches in terms of quality and computational cost.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2159}
}

@InProceedings{WiNLP-076,
  author    = {Gutierrez-Vasques, Ximena},
  title     = {Exploring bilingual lexicon extraction for Spanish-Nahuatl},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {208--211},
  abstract  = {In NLP, working with low-resource languages implies several challenges due to
	the lack of language technologies and digitally available corpus. Traditional
	methods need to be adapted in order to deal with the scarcity.
	Our case of study is the language pair Spanish-Nahuatl, which is spoken in the
	same country but the languages are very distant from each others (IndoEuropean
	and Uto-Aztecan language families).  We explore bilingual lexicon extraction
	for this language pair. However, there are several issues to address, e.g., it
	is difficult to obtain corpora, the languages have different morphological and
	syntactical phenomena, it is not easy to find standardized annotated resources
	for Nahuatl.
	We conjecture that in order to extract bilingual lexicon, it is not only
	important to pay attention on the methods but also in the morphology of the
	languages. We perform morphological normalization as a way to counteract the
	negative effect of a small parallel corpus of morphological rich languages. We
	measure the impact of dealing with morphology in the task of finding
	translation pairs.
	Our approach tries to be as unsupervised as possible, since it is difficult to
	rely on Nahuatl resources, due to the lack of orthographic norm, big dialectal
	variation and scarcity of resources.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2160}
}

@InProceedings{WiNLP-077,
  author    = {Kochkina, Elena  and  Liakata, Maria  and  Augenstein, Isabelle},
  title     = {Sequential Approach to Rumour Stance Classification},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {212--214},
  abstract  = {Rumour stance classification is a task that involves identifying the attitude
	of Twitter users towards the truthfulness of the rumour they are discussing.
	Stance classification is considered to be an important step towards rumour
	verification, therefore performing well in this task is expected to be useful
	in debunking false rumours. In this work we classify a set of Twitter posts
	discussing rumours into either supporting, denying, questioning or commenting
	on the underlying rumours. We propose an LSTM-based sequential model that,
	through modelling the conversational structure of tweets, obtains
	state-of-the-art accuracy on the SemEval-2017 RumourEval dataset.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2161}
}

@InProceedings{WiNLP-079,
  author    = {Guellil, Imane  and  Azouaou, Faical  and  Abbas, Mourad},
  title     = {Comparison between Neural and Statistical translation after transliteration of Algerian Arabic Dialect},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {215--219},
  abstract  = {Research on Arabic Dialect Treatment has recently become important in the
	literature. Although most work on these dialects considers only the messages or
	the portion of text written in Arabic letters, another style of writing has
	emerged on social media. This style is known by Arabizi and combines between
	Latin letters and numbers. To address this emergent problem in the context of
	automatic translation, we present an Arabic dialect translation system composed
	by two modules: Transliteration and translation. We develop each module with a
	statistical and a neural model. To test our system, we used the Algerian
	portion of a multi-dialectal Arabic corpus named PADIC. Experimental results
	show that a good transliteration improves the translation results. Moreover,
	the neural transliteration gives better results than the statistical
	transliteration. However, the statistical translation still gives better
	results that the neural translation.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2162}
}

@InProceedings{WiNLP-080,
  author    = {Ananya, Ananya  and  Singh, Sameer},
  title     = {How Biased Are We? Automated Detection of Gendered Language},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {220--223},
  abstract  = {Detecting the different ways in which gender bias is encoded in language is an
	important and challenging task, however, it currently requires a manual
	description of how gendered language is expressed. Apart from requiring
	considerable effort for each language/domain/author, such an analysis does not
	provide a quantifiable measure of this bias, nor is it identifed at the
	sentential level. Based on the intuition that gendered language is indicative
	of the gender of its mentions, we leverage syntactic and semantic constructions
	to develop an unsupervised classifier to predict the gender of any mention from
	its context. Such a classifier is then used on unseen text to predict the
	gender of its mentions, the confidence of which indicates the level of biasin
	the text. We present preliminary implementation and results in this extended
	abstract.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2163}
}

@InProceedings{WiNLP-082,
  author    = {Tatman, Rachael},
  title     = {\#MAGA or \#TheResistance: Classifying Twitter users' political affiliation without looking at their words or friends},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {224--226},
  abstract  = {Previous NLP work attempting to classify Twitter users' political affiliation
	relied predominantly on lexical features and information users' social
	networks. However, sociolinguistic work on the expression of political identity
	has found that sub-lexical features are also important reflexes of political
	identity. This study shows that it is possible to achieve above-chance
	classification of Twitter users based on only three features: how much
	punctuation and capitalization an account uses and what proportion of their
	tweets are replies.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2164}
}

@InProceedings{WiNLP-083,
  author    = {Almeida, Hayda  and  Briand, Antoine  and  Meurs, Marie-Jean},
  title     = {Predicting Depression from User-generated Content in Social Media},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {227--229},
  abstract  = {This paper presents an ensemble approach to predict early risk of depression
	from user-generated content in social media. 
	Supervised learning and information retrieval methods were used to estimate the
	risk of depression for a user given the content of its posts in small reddit. 
	The experiments show that combining information retrieval and machine learning
	approaches gives the best results.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2165}
}

@InProceedings{WiNLP-084,
  title = {User Trust When Machine Translation Goes Wrong},
  author = {Martindale, Marianne and Carpuat, Marine},
  sortname = {Martindale, Marianne and Carpuat, Marine}}

@InProceedings{WiNLP-085,
  author    = {Jacobs, Cassandra L.},
  title     = {The role of domain in learning about referents},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {230--232},
  abstract  = {A number of natural language processing tasks such as knowledge representation,
	coreference resolution, or other information retrieval tasks depend on learning
	good representations of real-world entities. Here we demonstrate that the
	language associated with different entities is highly sensitive to the domain.
	By analyzing the entities associated with requests, reviews, and descriptions,
	we can learn entity representations that are stable across different textual
	domains, but that the entities that are relevant to a request are not
	necessarily easily translated into entities that are similar based on how they
	are described in reviews.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2166}
}

@InProceedings{WiNLP-086,
  author    = {Hu, Zhichao  and  Walker, Marilyn},
  title     = {Measuring Personality Adaptation in Task-Oriented Dialogs},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {233--235},
  abstract  = {Decades of research on human communication provides substantial evidence that
	human conversants in dialogue adjust their language to their conversational
	partner: conversants adapt to particular ways of referring to things, mimic the
	partner's lexical diversity and entrain to their vocabulary choices.
	Conversants also adapt to each other's personality through gestures, however,
	few work has addressed linguistic adaptation in terms of personality. In this
	paper, we propose a method to calculate adaptation score for a dialog. We apply
	our method on two task oriented dialog corpora and show that as the dialogs
	progress, the adaptation score for extraversion related LIWC features (1)
	displays a distinct trend compared to all LIWC features, and (2) differs
	between two corpora.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2167}
}

@InProceedings{WiNLP-087,
  author    = {Johnson, Kristen Maria},
  title     = {Modeling of Political Discourse on Twitter},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {236--238},
  abstract  = {Our works focus on the application of NLP methods for the analysis of political
	discourse on Twitter. Our guiding intuition is that modeling the language used
	on Twitter alone is not enough for the most accurate prediction possible.
	Therefore, we explore how weakly supervised models can be constructed to
	leverage both language and behaviors of politicians on Twitter to identify
	stance and framing patterns from the discourse. By incorporating behavioral
	features, such as similar temporal information, stances on current and future
	political issues, as well as the frames used to express these issues, can be
	determined with higher accuracy than what is possible with language based
	models alone.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2168}
}

@InProceedings{WiNLP-091,
  author    = {Reed, Lena},
  title     = {Collecting Artwork Descriptions Through Dialogue},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {239--241},
  abstract  = {Dialogue systems are becoming smarter and more common, and they are being used
	for an increasing number of purposes.  We are creating a task oriented art
	chatbot that would allow people to ask questions about artworks and have a
	conversation about art. Using data obtained from a crowdsourced database and
	dialogues  between humans through a messenger application, we have started
	collecting data that we will use to create the database for this systems and
	help it learn conversational techniques to employ in natural communication on
	this topic.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2169}
}

@InProceedings{WiNLP-092,
  author    = {Guellil, Imane  and Azouaou, Faical},
  title     = {Bilingual Lexicon for Algerian Arabic Dialect Treatment in Social Media},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {242--245},
  abstract  = {During the last ten years, the interest of Arabic dialects treatment has
	greatly increased. This increase is mainly attributed to the large use of these
	dialects in social media. The works on Arabic dialects can be classified into
	four categories: basic analysis, resource construc-tion, identification of
	Arabic dialects and semantic analysis on Arabic dialects. In this paper, we
	address the problem of Arabic lexicon construction. More specifically, we focus
	on Algerian dialect which is spoken by more than 40 million people. Our
	approach consists of three distinct steps: 1) Constructing a dialect lexicon
	and extracting the most intense words from a sentiment lexicon; 2) Merging two
	lexicons (a dialect and a sentiment lexicon) and replacing some letters with
	the most used letters in social media; 3) Enriching a lexicon that takes into
	account spelling variations of words specific to a social media domain. As a
	result, we ended up expanding the original lexicon of 1144 words to a new
	lexicon 25086 words.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2170}
}

@InProceedings{WiNLP-093,
  author    = {Rakshit, Geetanjali  and  Bowden, Kevin  and  Reed, Lena  and  Misra, Amita  and  Walker, Marilyn},
  title     = {Debbie, the Debate Bot of the Future},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {246--248},
  abstract  = {Chatbots are a rapidly expanding application of dialogue systems with companies
	switching to bot services for customer support, and new applications for users
	interested in casual conversation.  One style of casual conversation is
	argument; many people love nothing more than a good argument. Moreover, there
	are a number of existing corpora of argumentative dialogues, annotated for
	agreement and disagreement, stance, sarcasm and argument quality. This paper
	introduces Debbie, a novel arguing bot, that selects arguments from
	conversational corpora, and aims to use them appropriately in context. We
	present an initial working prototype of Debbie, with some preliminary
	evaluation and describe future work.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2171}
}

@InProceedings{WiNLP-094,
  author    = {Rijhwani, Shruti  and  Sequiera, Royal  and  Choudhury, Monojit  and  Bali, Kalika},
  title     = {Generalized and Unsupervised Word-Level Language Detection},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {249--252},
  abstract  = {Word-level language detection is necessary for processing code-switched text,
	where multiple languages could be mixed within a sentence. Existing models are
	restricted to identifying code-switching between two specific languages and
	fail in real-world scenarios as text input rarely has a priori information on
	the languages used. We present a novel unsupervised word-level language
	detection technique for code-switched text in an arbitrarily large set of
	languages, which does not require any manually annotated training data. Our
	experiments with monolingual and code-switched tweets in seven languages show a
	74\% relative error reduction in word-level language labeling with respect to
	competitive baselines.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2172}
}

@InProceedings{WiNLP-095,
  author    = {Andersson, Linda  and  Rekabsaz, Navid  and  Hanbury, Allan},
  title     = {Automatic Query Expansion for Patent Passage Retrieval using Paradigmatic and Syntagmatic Information},
  booktitle = {Proceedings of the First Women and Underrepresented Groups in NLP Workshop},
  month     = {July},
  year      = {2017},
  address   = {Vancouver, BC, Canada},
  publisher = {Association for Computational Linguistics},
  pages     = {253--257},
  abstract  = {Even with my 12 years of working with patent data, I know just a few
	computational linguistics that have worked or still work with patent text.
	However, I think we can contribute a lot to this rather complex text domain,
	which is what I tried to summarize in this extended abstract. 
	In this paper we present one of the most successful patent passage retrieval
	method combining linguistic knowledge, NLP and statistic for query generation.
	Part of this work have previously been published in different conferences, but
	in this paper we merge the outcome of two publications to explore automatic
	query expansion by identifying domain specific lexical-semantic relation with
	lexico-syntactic-patterns in combination with distributional semantic.},
  url       = {http://www.aclweb.org/anthology/W/W17/W17-2173}
}

